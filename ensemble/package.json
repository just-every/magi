{
    "name": "@magi-system/ensemble",
    "version": "4.0.0",
    "description": "Shared model-provider utilities for MAGI System",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": ["dist", "package.json"],
    "scripts": {
        "build": "tsc -p .",
        "clean": "rimraf dist",
        "prepare": "npm run build"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./model_providers/*": {
            "import": "./dist/model_providers/*.js",
            "types": "./dist/model_providers/*.d.ts"
        },
        "./utils/*": {
            "import": "./dist/utils/*.js",
            "types": "./dist/utils/*.d.ts"
        }
    },
    "keywords": ["llm", "openai", "anthropic", "model-providers"],
    "author": "MAGI System",
    "license": "MIT",
    "dependencies": {
        "@anthropic-ai/sdk": "^0.51.0",
        "@google/genai": "^0.12.0",
        "openai": "^4.100.0",
        "dotenv": "^16.5.0",
        "sharp": "^0.32.0"
    },
    "devDependencies": {
        "typescript": "^5.8.2",
        "@types/node": "^22.14.0"
    }
}
