{
    "name": "magi",
    "version": "1.0.0",
    "description": "MAGI System TypeScript Implementation",
    "main": "dist/magi.js",
    "type": "module",
    "scripts": {
        "build": "tsc -b",
        "build:docker": "tsc --outDir /app/dist",
        "start": "node --no-deprecation --experimental-vm-modules dist/magi.js",
        "start:docker": "npm run build:docker && node --no-deprecation --experimental-vm-modules /app/dist/magi.js",
        "dev": "nodemon --watch src --watch ../common --ext ts --exec 'npm run build && npm start'",
        "dev:fast": "tsx watch src/magi.ts",
        "lint": "tsc --noEmit && eslint \"src/**/*.ts\"",
        "lint:fix": "tsc --noEmit && eslint \"src/**/*.ts\" --fix",
        "test": "node dist/index.js",
        "test:prompt": "node dist/index.js",
        "browser": "npm run build && node dist/test-browser.js",
        "browse": "npm run build && node dist/test-browser.js -u"
    },
    "license": "ISC",
    "dependencies": {
        "@anthropic-ai/sdk": "^0.53.0",
        "@google/genai": "^1.4.0",
        "@just-every/ensemble": "^0.2.35",
        "@just-every/task": "^0.2.5",
        "@napi-rs/canvas": "^0.1.71",
        "@types/turndown": "^5.0.5",
        "axios": "^1.9.0",
        "chrome-remote-interface": "^0.33.3",
        "dotenv": "^16.5.0",
        "esbuild": "^0.25.5",
        "glob": "^11.0.2",
        "jsdom": "^26.1.0",
        "node-pty": "^1.0.0",
        "openai": "^5.2.0",
        "sharp": "^0.34.2",
        "strip-ansi": "^7.1.0",
        "ts-node": "^10.9.2",
        "turndown": "^7.2.0",
        "undici-types": "^7.10.0",
        "uuid": "^11.1.0",
        "ws": "^8.18.2"
    },
    "overrides": {
        "tar-fs": "^2.1.3"
    },
    "devDependencies": {
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.15.30",
        "@types/pg": "^8.15.4",
        "@types/ws": "^8.18.1",
        "@typescript-eslint/eslint-plugin": "^8.34.0",
        "@typescript-eslint/parser": "^8.34.0",
        "eslint": "^9.28.0",
        "nodemon": "^3.1.10",
        "pg": "^8.16.0",
        "tsx": "^4.19.4",
        "typescript": "^5.8.3",
        "vitest": "^3.2.3"
    }
}
