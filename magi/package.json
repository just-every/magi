{
    "name": "magi",
    "version": "1.0.0",
    "description": "MAGI System TypeScript Implementation",
    "main": "dist/magi.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "node dist/magi.js",
        "dev": "npm run build && npm start",
        "lint": "tsc --noEmit && eslint \"src/**/*.ts\"",
        "lint:fix": "tsc --noEmit && eslint \"src/**/*.ts\" --fix",
        "test": "node dist/index.js",
        "test:prompt": "node dist/index.js",
        "browser": "npm run build && node dist/test-browser.js",
        "browse": "npm run build && node dist/test-browser.js -u"
    },
    "license": "ISC",
    "dependencies": {
        "@anthropic-ai/sdk": "^0.17.1",
        "@google/genai": "^0.7.0",
        "@napi-rs/canvas": "^0.1.69",
        "@types/turndown": "^5.0.5",
        "axios": "^1.6.8",
        "chrome-remote-interface": "^0.33.3",
        "dotenv": "^16.4.7",
        "jsdom": "^26.0.0",
        "node-pty": "^1.0.0",
        "openai": "^4.88.0",
        "sharp": "^0.33.5",
        "strip-ansi": "^7.1.0",
        "turndown": "^7.2.0",
        "undici-types": "^7.5.0",
        "uuid": "^11.1.0",
        "ws": "^8.18.1"
    },
    "overrides": {
        "tar-fs": "^2.1.2"
    },
    "devDependencies": {
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.13.10",
        "@types/ws": "^8.18.0",
        "@typescript-eslint/eslint-plugin": "^8.26.1",
        "@typescript-eslint/parser": "^8.26.1",
        "eslint": "^9.22.0",
        "typescript": "^5.8.2",
        "vitest": "^3.1.1"
    }
